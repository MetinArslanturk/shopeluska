{
    "extends": [
      "react-app",
      "airbnb",
      "airbnb/hooks",
      "plugin:jsx-a11y/recommended",
      "prettier",
      "prettier/react"
    ],
    "plugins": [
      "jsx-a11y",
      "prettier"
    ],
    "rules": {

      "import/no-named-as-default": 0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "prettier/prettier": [
        "error", {
          "semi": true,
          "singleQuote": true,
          "tabWidth": 4
        }
      ]
    }
  }